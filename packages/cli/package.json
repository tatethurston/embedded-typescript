{
  "name": "embedded-typescript",
  "version": "0.1.0",
  "description": "Type safe TypeScript templates",
  "license": "MIT",
  "author": "Tate <tatethurston@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tatethurston/embedded-typescript"
  },
  "bin": {
    "ets": "./dist/index.js"
  },
  "files": [
    "dist/index.js"
  ],
  "scripts": {
    "build": "yarn clean && esbuild src/index.ts --platform=node --target=node16 --bundle --outfile=dist/index.js && cp types.ts dist/index.d.ts",
    "clean": "rm -rf dist",
    "prepack": "yarn run build && cp ../../README.md .",
    "postpack": "rm README.md",
    "postversion": "git add --all package.json && git push && git push --tags"
  },
  "types": "dist",
  "dependencies": {
    "prettier": "^2.7.1"
  },
  "devDependencies": {
    "esbuild": "^0.11.20"
  },
  "keywords": [
    "embedded typescript",
    "ets",
    "type safe string",
    "type safe template",
    "type safe templating",
    "typescript template",
    "typescript templating"
  ]
}
