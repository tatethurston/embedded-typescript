{
  "name": "embedded-typescript",
  "description": "Type safe embedded TypeScript templates",
  "license": "MIT",
  "author": "Tate <tatethurston@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tatethurston/embedded-typescript"
  },
  "version": "0.0.6",
  "bin": {
    "ets": "./dist/index.js"
  },
  "files": [
    "dist/index.js"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --platform=node --target=node12 --bundle --outfile=dist/index.js",
    "clean": "rm -rf dist",
    "version": "yarn run build && git add -A package.json",
    "postversion": "git push && git push --tags"
  },
  "types": "dist",
  "devDependencies": {
    "esbuild": "^0.11.16"
  },
  "keywords": [
    "embedded",
    "engine",
    "template",
    "templating",
    "type safe",
    "typescript"
  ]
}
